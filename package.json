{
    "name": "better-auth-waitlist",
    "type": "module",
    "version": "2.1.3",
    "packageManager": "pnpm@10.13.1",
    "description": "A Better Auth plugin for waitlist functionality",
    "author": "Seifeddine LARGAT <seifeddine.largat@outlook.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/LargatSeif/better-auth-waitlist"
    },
    "keywords": [
        "better-auth",
        "waitlist",
        "plugin",
        "authentication"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/cjs/index.d.cts",
                "default": "./dist/cjs/index.cjs"
            }
        },
        "./client": {
            "import": {
                "types": "./dist/esm/client.d.ts",
                "default": "./dist/esm/client.js"
            },
            "require": {
                "types": "./dist/cjs/client.d.cts",
                "default": "./dist/cjs/client.cjs"
            }
        }
    },
    "main": "dist/cjs/index.cjs",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && publint --strict",
        "test": "vitest run --watch",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
        "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
        "changeset": "changeset",
        "version": "changeset version",
        "checktypes": "tsc --noEmit",
        "check:all": "pnpm checktypes && pnpm lint:fix && pnpm test",
        "publish": "pnpm build && changeset publish"
    },
    "peerDependencies": {
        "better-auth": ">=1.2.8"
    },
    "dependencies": {
        "@better-auth-kit/tests": "^0.2.0",
        "zod": "^4.0.16"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^5.2.0",
        "@changesets/cli": "^2.29.5",
        "@eslint/js": "^9.33.0",
        "@tanstack/config": "^0.20.0",
        "@tanstack/eslint-config": "^0.3.0",
        "@types/better-sqlite3": "^7.6.13",
        "@typescript-eslint/eslint-plugin": "^8.39.0",
        "@typescript-eslint/parser": "^8.39.0",
        "@vitest/ui": "^3.2.4",
        "better-sqlite3": "^12.2.0",
        "eslint": "^9.33.0",
        "eslint-plugin-format": "^1.0.1",
        "publint": "^0.3.12",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2",
        "vite": "^7.1.1",
        "vitest": "^3.2.4"
    },
    "publishConfig": {
        "access": "public"
    }
}
